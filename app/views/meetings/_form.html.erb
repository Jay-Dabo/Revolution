<%= simple_form_for(@meeting) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :instance_id %>
    <%= f.input :group_id %>
    <%= f.input :room_id %>
    <%= f.input :real_date %>
    <%= f.input :comments %>
    <%= f.input :num_marked %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
