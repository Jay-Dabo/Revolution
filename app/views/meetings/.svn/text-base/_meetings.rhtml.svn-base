<strong>Meetings for This Instance</strong>
<table class="list">
	<tr>
		<th>Group</th>
		<th>Room</th>
	</tr>
	
	<% @instance.meetings.sort_by {|m| m.group.name rescue nil}.each do |meeting| %>
	<tr>
		<td><%= meeting.group.name rescue nil %></td>
		<td><%= meeting.room.name rescue nil %></td>
	</tr>
			 	

	<% end %>
</table>

