<% form_tag :action => 'set_choices', :id => 5 do %>

<p><label>Curriculum Choice</label><br>
<%= text_field_with_auto_complete :curriculum_choice, :name %></p>

<p><label>Curriculum Cost</label><br>
<%= text_field_with_auto_complete :curriculum_cost, :name %></p>

<p><label>Special Requirement</label><br>
<%= text_field_with_auto_complete :special_requirement, :name %></p>



<%= submit_tag "Set" %>
<% end %>

<br><br>

<%= debug(@special_requirement) rescue nil %>