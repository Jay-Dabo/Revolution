
<table class="list">
	<tr>
		<th>Number</th>
		<th>Primary?</th>
		<th>Type</th>
		<th>Carrier</th>
		<th>Comments</th>
	</tr>
	<% @entity.phones.collect do |phone| %>
	<tr class="<%= cycle "even", "odd" %>">
		<td><%= number_to_phone phone.number, {:area_code => true} %></td>
		<td><%= check_box_tag 'phone', phone[:primary], phone[:primary], :disabled => true %></td>
		<td><%= phone.comm_type.name rescue nil %></td>
		<td><%= phone.sms_setup.carrier_name rescue nil %></td>
		<td><%= phone.comments %></td>	
	</tr>
	<% end %>
</table>