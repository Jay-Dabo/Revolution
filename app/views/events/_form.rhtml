<%= error_messages_for 'event' %>

<!--[form:event]-->
<p><label for="event_date">Date</label><br/>
<%= date_field 'event', 'date'  %></p>

<% @event_types = EventType.find(:all) %>
<p><label for="event_type">Event Type</label><br/>
	<%= collection_select 'event', 'event_type_id', @event_types, 'id', 'name' %>
</p>

<p><label for="event_name">Name</label><br/>
<%= text_field 'event', 'name'  %></p>
