<h1>Listing auto_groups</h1>

<table>
  <tr>
  <% for column in AutoGroup.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for auto_group in @auto_groups %>
  <tr>
  <% for column in AutoGroup.content_columns %>
    <td><%=h auto_group.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => auto_group %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => auto_group %></td>
    <td><%= link_to 'Destroy', { :action => 'destroy', :id => auto_group }, :confirm => 'Are you sure?', :method => :post %></td>
  </tr>
<% end %>
</table>

<%= link_to 'Previous page', { :page => @auto_group_pages.current.previous } if @auto_group_pages.current.previous %>
<%= link_to 'Next page', { :page => @auto_group_pages.current.next } if @auto_group_pages.current.next %> 

<br />

<%= link_to 'New auto_group', :action => 'new' %>
