<h3>Listing Sign_ups</h3>

<table class="twelve responsive">
  <thead>
	  <tr>
			<th>First_name</th>
			<th>Last_name</th>
			<th>Gender</th>
			<th>Phone</th>
	    <th></th>
	  </tr>
  </thead>
  <tbody>
	<% @sign_ups.each do |sign_up| %>
	  <tr>
	    <td><%= sign_up.first_name %></td>
	    <td><%= sign_up.last_name %></td>
	    <td><%= sign_up.gender %></td>
	    <td><%= sign_up.phone %></td>
	    <td><%= link_to '', edit_sign_up_path(sign_up), title: 'edit', class: "general-enclosed foundicon-edit" %>&nbsp;&nbsp;&nbsp;
	    	<%= link_to '', sign_up, method: :delete, data: { confirm: 'Are you sure?' }, title: 'destroy', class: "general-enclosed foundicon-remove" %>
	   </td>
	  </tr>
	<% end %>
  </tbody>
</table>

<br />

<%= link_to "New Sign_up", new_sign_up_path, class: "button radius"  %>
