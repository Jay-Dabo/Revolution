<%= simple_form_for(@attendance) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :person_id %>
    <%= f.input :meeting_id %>
    <%= f.input :checkin_time %>
    <%= f.input :checkout_time %>
    <%= f.input :checkin_type_id %>
    <%= f.input :security_code %>
    <%= f.input :call_number %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
