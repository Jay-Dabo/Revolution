<%= simple_form_for(@leadership) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :leadable_id %>
    <%= f.input :leadable_type %>
    <%= f.input :type %>
    <%= f.input :person_id %>
    <%= f.input :title %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
