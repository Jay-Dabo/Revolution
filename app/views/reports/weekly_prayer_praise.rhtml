<h3 style="display:inline">Weekly Prayer Requests & Praise Reports <%= Time.now.strftime('%m/%d/%Y') %></h3>
<hr>

<br><br>

<% if ! @stamps.empty? %>
<% @stamps.each do |c| %>
<div id="contact-type" style="float:none;clear:both;page-break-inside:avoid;">
	<label><%= c.stamp %></label>
	<div id="person-info" style="margin-left:3em;">
		<I><%# at.household_people.collect {|p| p.full_name + '(' + p.age.to_s + ')'}.to_sentence %></I>
	</div>
	<hr>
	<%= render :partial => "contact_w_type", :collection => Contact.weekly_by_stamp_and_type_name(c.stamp,["Praise Report","Prayer Request"]) %>
</div><br><br>
<% end %>


<% else %>
<div id="message-black">
	There are no contacts.
</div>
<% end %>