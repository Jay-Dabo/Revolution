<h1>Listing follow_up_types</h1>

<table>
  <tr>
  <% for column in FollowUpType.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for follow_up_type in @follow_up_types %>
  <tr>
  <% for column in FollowUpType.content_columns %>
    <td><%=h follow_up_type.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => follow_up_type %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => follow_up_type %></td>
    <td><%= link_to 'Destroy', { :action => 'destroy', :id => follow_up_type }, :confirm => 'Are you sure?', :method => :post %></td>
  </tr>
<% end %>
</table>

<%= link_to 'Previous page', { :page => @follow_up_type_pages.current.previous } if @follow_up_type_pages.current.previous %>
<%= link_to 'Next page', { :page => @follow_up_type_pages.current.next } if @follow_up_type_pages.current.next %> 

<br />

<%= link_to 'New follow_up_type', :action => 'new' %>
