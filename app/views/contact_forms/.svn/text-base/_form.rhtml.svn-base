<p><label for="contact_form_created_at">Created at</label><br/>
<%= datetime_select 'contact_form', 'created_at'  %></p>

<p><label for="contact_form_updated_at">Updated at</label><br/>
<%= datetime_select 'contact_form', 'updated_at'  %></p>
