<% @attendances.each do |attendance| %>
	<% if attendance.person.has_allergies? %>
		<%= render :partial => '/checkin/stickers/child_sticker_allergies', locals: { attendance: attendance } %>
	<% else %>
		<%= render :partial => '/checkin/stickers/child_sticker', locals: { attendance: attendance } %>
	<% end %>
<% end %>