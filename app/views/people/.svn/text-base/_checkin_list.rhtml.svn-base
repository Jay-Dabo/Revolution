

<% @household.people.sort_by(&:sort_order).each do |person| %>
<div id="person-row" class="<%= cycle("even", "odd") %>" style="width:100%;">
<div id="person-<%= person.id %>" style="width:100%;"> <% @person = person %>
		
<%= render :partial => "people/checkin_person", :locals => {:person => person} %>

</div>
</div>
<% end %>






