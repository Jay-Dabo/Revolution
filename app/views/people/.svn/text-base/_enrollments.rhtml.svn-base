<div style="float:none;clear:both;">
	
<fieldset><legend><strong>Enrollments</strong></legend>

<table class="list">
	<tr>
		<th>Group</th>
		<th>Start Date</th>
	</tr>
	<% @person.enrollments.each do |e| %>
	<tr class="<%= cycle "even", "odd" %>">
		<td><%= e.group.name rescue nil %></td>
		<td><%= e.created_at.to_date.strftime('%m/%d/%Y') rescue nil %></td>	
	</tr>
	<% end %>
</table>
</fieldset>

</div>