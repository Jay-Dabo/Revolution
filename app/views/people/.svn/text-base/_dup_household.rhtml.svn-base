<% unless @duplicate.household.nil? %>
<div>
	<strong>Household Info</strong> (moved from)
</div>
<div>
	<span><%= @duplicate.household.household_name rescue nil %> (#<%= @duplicate.household.id rescue nil %>)</span><br>
	<span><%= @duplicate.household.address_block %></span><br>
	<span><strong>Household People:</strong> <%= @duplicate.household.people.collect {|p| p.first_name}.to_sentence rescue nil %></span>
</div>
<% end %>