<h3 class='subheader'>Listing Application Settings</h3>

<table class='twelve responsive'>
	<tr>
		<th>Name</th>
		<th>Value</th>
	</tr>
	<% @settings.each do |setting| %>
		<tr>
			<td><%= setting.key %></td>
			<td><%= setting.value %></td>
		</tr>
	<% end %>
</table>

<%= link_to 'New Setting', new_admin_setting_path, class: 'button radius' %>