<%= error_messages_for 'contribution' %>

<!--[form:contribution]-->
<p><label for="contribution_total">Total</label><br/>
<%= text_field 'contribution', 'total'  %></p>

<p><label for="contribution_created_at">Created at</label><br/>
<%= datetime_select 'contribution', 'created_at'  %></p>

<p><label for="contribution_updated_at">Updated at</label><br/>
<%= datetime_select 'contribution', 'updated_at'  %></p>

<p><label for="contribution_created_by">Created by</label><br/>
<%= text_field 'contribution', 'created_by'  %></p>

<p><label for="contribution_updated_by">Updated by</label><br/>
<%= text_field 'contribution', 'updated_by'  %></p>

<p><label for="contribution_comments">Comments</label><br/>
<%= text_area 'contribution', 'comments'  %></p>

<p><label for="contribution_check_num">Check num</label><br/>
<%= text_field 'contribution', 'check_num'  %></p>

<p><label for="contribution_date">Date</label><br/>
<%= datetime_select 'contribution', 'date'  %></p>
