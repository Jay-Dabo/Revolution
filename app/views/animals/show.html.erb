<%= four_up_two_up_widget_box("Basic Information") do %>
	<li class="field">Name:</li>
	<li><%= @animal.name %></li>
	<li class="field">Updated_by:</li>
	<li><%= @animal.updated_by %></li>
	<li class="field">Created_by:</li>
	<li><%= @animal.created_by %></li>
<% end  %>
  

<div class="form-actions">
  <%= button_icon 'Index', animals_path, 'list', nil, 'small round'  %>
  <%= button_icon 'Edit', edit_animal_path(@animal), 'edit', nil, 'small round' %>
</div>