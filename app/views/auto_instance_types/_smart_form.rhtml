<%= error_messages_for 'auto_instance_type' %>

<% @instance_types = InstanceType.find(:all) %>
<% @auto_instances = EventType.find(@event_type).instance_types %>
<% @available_instance_types = (@instance_types - @auto_instances) %>
<%= collection_select('auto_instance_type', 'instance_type_id', @available_instance_types, 'id', 'name') %>

