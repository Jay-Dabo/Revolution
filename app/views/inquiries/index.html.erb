<h3>Listing Inquiries</h3>

<table class="twelve responsive">
  <thead>
	  <tr>
			<th>Person_id</th>
			<th>Group_id</th>
			<th>Created_by</th>
			<th>Updated_by</th>
	    <th></th>
	  </tr>
  </thead>
  <tbody>
	<% @inquiries.each do |inquiry| %>
	  <tr>
	    <td><%= inquiry.person_id %></td>
	    <td><%= inquiry.group_id %></td>
	    <td><%= inquiry.created_by %></td>
	    <td><%= inquiry.updated_by %></td>
	    <td><%= link_to '', edit_inquiry_path(inquiry), title: 'edit', class: "general-enclosed foundicon-edit" %>&nbsp;&nbsp;&nbsp;
	    	<%= link_to '', inquiry, method: :delete, data: { confirm: 'Are you sure?' }, title: 'destroy', class: "general-enclosed foundicon-remove" %>
	   </td>
	  </tr>
	<% end %>
  </tbody>
</table>

<br />

<%= link_to "New Inquiry", new_inquiry_path, class: "button radius"  %>
